            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".danhsachchude.DanhSachChuDeActivity"
            android:label="@string/title_activity_danh_sach_chu_de" >
        </activity>
        <activity
            android:name=".chude.ChuDeActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_chu_de" >
        </activity>
        <activity
            android:name=".kiemtra.KiemTraActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_kiem_tra" >
        </activity>
    </application>

</manifest>
